@{
    ViewData["Title"] = "Send Data to Odoo";
}

<h1>@ViewData["Title"]</h1>

<form asp-controller="Exportaciones" asp-action="EnviarDatosOdoo" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="file">Choose file:</label>
        <input type="file" class="form-control" id="file" name="file">
    </div>
    <div class="form-group">
        <label for="batchDate">Batch Date:</label>
        <input type="date" class="form-control" id="batchDate" name="batchDate">
    </div>
    <button type="submit" class="btn btn-primary">Enviar Datos</button>
</form>